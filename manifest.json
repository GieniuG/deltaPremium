	"manifest_version":3,
	"version":"1.0",
	"name":"DeltaPremium",
	"permissions":["activeTab"],
	"icons":{
		"48": "icon.png"
	},
	"content_scripts": [
		{
			"matches":["*://delta.pk.edu.pl/*"],
			"js":["content.js"]
		},{
            "matches":["*://learningapps.org/*"],
            "all_frames":true,
            "js":["lr.js"]
        }
	]
}
